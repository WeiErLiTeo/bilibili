{
  "name": "Flutter (Codespaces minimal)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "flutter doctor -v",
  "forwardPorts": [8080],
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  }
}
